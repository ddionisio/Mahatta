{
	file sounds\dooropen.ogg
	refDist 100
	rollOff 2.5
	maxDist 3000
}

fan
{
	file sounds\fan.ogg
	looping
	refDist 100
	rollOff 2.5
	maxDist 3000	
}

run
{
	file sounds\run.ogg
	local
	looping
}

spin
{
	file sounds\spin.ogg
}

strafe
{
	file sounds\strafe.ogg
	local
	looping
}

swish
{
	file sounds\swish.ogg
}

truck
{
	file sounds\truck.ogg
	looping
	refDist 100
	rollOff 2.5
	maxDist 2500
}

vent
{
	file sounds\vent.ogg
	looping
