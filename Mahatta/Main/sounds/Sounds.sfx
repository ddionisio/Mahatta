bullet_fire
{
	file sounds\smash.ogg
	local
	precache
}

bullet_bounce
{
	file sounds\spin.ogg
	precache
	refDist 60
	rollOff 2.75
	maxDist 3000	
}

bullet_explode
{
	file sounds\explode.ogg
	precache
	refDist 60
	rollOff 2.75
	maxDist 3000	
}


//format of sound library:
//<name>
//{
//		file <string>		//the sound file
//		volume <int>
//		local				//sound is 2D
//		looping				//sound loops
//		no_cull				//cannot be culled by the map
//		precache			//sound data is loaded once at the start
//		refDist <float>		//reference distance
//		rollOff <float>		//roll off
//		maxDist <float>		//max distance